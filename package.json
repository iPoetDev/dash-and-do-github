{
  "scripts": {
    "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "test": "standard && node **.js | snazzy",
    "test-fix": "standard --fix && node **.js | snazzy",
    "validate": "html-validator docs/*.html flyer.html --exclude=build,tmp --delay=200",
    "one-folder": "html-validator docs --exclude=build,tmp --delay=200",
    "all": "html-validator --quiet --exclude=build,tmp --delay=200",
    "continue": "html-validator --quiet --continue --exclude=build,tmp --delay=200",
    "server": "live-server --port=8005 --host=127.0.0.1  --wait=250 ./"
  },
  "devDependencies": {
    "editorconfig": "^2.0.0",
    "eslint": "^8.48.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-module-utils": "^2.8.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-scope": "^7.2.2",
    "eslint-utils": "^3.0.0",
    "htmlhint": "^1.1.4",
    "htmx": "^0.0.2",
    "htmx.org": "^1.9.5",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "standard": "^17.1.0",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "stylelint-csstree-validator": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "w3c-html-validator": "^1.4.0"
  },
  "browserslist": [
    ">0.3%, defaults and supports es6-module",
    "maintained node versions"
  ],
  "dependencies": {
    "autoprefixer": "^10.4.15",
    "browserslist": "^4.21.10",
    "live-server": "^1.2.2",
    "snazzy": "^9.0.0"
  },
  "standard": {
    "ignore": [
      "**/out/",
      "/lib/*",
      "tmp.js"
    ]
  }
}
